{
  "name": "brave-capture",
  "version": "1.0.0",
  "description": "Chrome extension for capturing and tracking CLM positions",
  "type": "module",
  "scripts": {
    "validate": "node scripts/validate-capture.js",
    "analyze": "node scripts/analyze-capture.js",
    "build:config": "node build-config.js",
